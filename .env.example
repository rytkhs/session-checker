# 対象URL
INITIAL_URL=https://example.com/dashboard  # ここに実際のURLを入力

# タイミング設定
MANUAL_LOGIN_WAIT_SECONDS=60
POST_ACCESS_WAIT_SECONDS=10
INITIAL_WAIT_TIME=55  # 55分
WAIT_TIME_INCREMENT=5  # 5分

# ユーザーエージェント
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36

# ファイル設定
COOKIES_FILE=saved_cookies.json
SESSION_FILE=selenium_session.pkl
CHROME_PROFILE_DIR=chrome_profile
